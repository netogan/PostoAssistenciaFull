@model IEnumerable<PostoAssistenciaFull.Models.Chamada>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DataChamada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataCriacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observação)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DataChamada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataCriacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observação)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.ChamadaId }) |
                @Html.ActionLink("Details", "Details", new { id=item.ChamadaId }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.ChamadaId })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
